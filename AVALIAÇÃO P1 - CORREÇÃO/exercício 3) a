#include <stdio.h>

int main() {
    double valor, celsius;

    printf("Digite o valor em graus Fahrenheit: \n");
    scanf("%lf", &valor);

    celsius = (valor - 32) * (5.0 / 9.0); 
    // Correção no formato da impressão: %.2lf
    printf("O valor em graus Celsius é: %.2lf\n", celsius); 

    return 0;
}
